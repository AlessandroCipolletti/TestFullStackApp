{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "filenames"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "no-unused-vars": "off",
    "import/no-unresolved": "error",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ],
    "filenames/match-exported": "error"
  }
}